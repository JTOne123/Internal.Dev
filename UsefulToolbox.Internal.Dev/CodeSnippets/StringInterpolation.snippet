<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Invariant String Interpolation</Title>
      <Author>Igor Eroshkin</Author>
      <Description>Code snippet for string interpolation with invariant culture</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>invariantsi</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>static System.FormattableString</Namespace>
        </Import>
      </Imports>
      <Declarations>
      </Declarations>
      <Code Language="csharp" Delimiter="$"><![CDATA[Invariant($$"$end$")]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>