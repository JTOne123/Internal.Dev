<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Import Test Helpers</Title>
      <Author>Igor Eroshkin</Author>
      <Description>Code snippet for importing test helpers</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>ithelpers</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>static UsefulToolbox.NUnit.Extensions.TestCaseHelpers</Namespace>
        </Import>
        <Import>
          <Namespace>static UsefulToolbox.NUnit.Extensions.ConstraintHelpers</Namespace>
        </Import>
        <Import>
          <Namespace>static UsefulToolbox.NSubstitute.Extensions.SubstituteHelpers</Namespace>
        </Import>
      </Imports>
      <Code Language="csharp" Delimiter="$"><![CDATA[$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>