<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>TestClass</Title>
      <Author>Igor Eroshkin</Author>
      <Description>Code snippet for test class</Description>
      <HelpUrl>
      </HelpUrl>
      <Shortcut>tc</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>NUnit.Framework</Namespace>
        </Import>
      </Imports>
      <Declarations>
        <Literal Editable="true">
          <ID>ClassName</ID>
          <ToolTip>Class name for testing</ToolTip>
          <Default>ClassName</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="csharp" Delimiter="$"><![CDATA[[TestFixture]
[TestOf(typeof($ClassName$))]
[Parallelizable]
public class $ClassName$Tests
{
    $end$       
}]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>